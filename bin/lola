#!/usr/bin/env python
import sys
import string
import random

VOWELS = [ 'a', 'e', 'i', 'o', 'u' ]
CONSONENTS = [ ]
for i in string.ascii_lowercase:
    if i not in VOWELS:
        CONSONENTS.append(i)

if sys.argv[1].isdigit():
    for i in range(int(sys.argv[1])):
        if not i % 2:
            print(random.choice(VOWELS), end='')
        else:
            print(random.choice(CONSONENTS), end='')
    print()

# vim ft=python3
